{
  "extends": ["next/core-web-vitals", "next/typescript", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "no-console": "warn",
    "no-mixed-spaces-and-tabs": "warn",
    "no-unused-vars": "warn",
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-unused-vars": "warn",
    "prettier/prettier": "warn"
  }
}
